<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD SQL Map Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <!-- http://www.mybatis.org/mybatis-3/configuration.html#settings 참고 -->
    <settings>
        <!-- query 결과 컬럼의 값이 null일 경우 result에 null로 setting할지 여무 -->
        <setting name="callSettersOnNulls" value="true" />
        <!-- null parameter 허용 -->
        <setting name="jdbcTypeForNull" value="NULL" />
    </settings>

    <typeAliases>
        <typeAlias type="com.px.init.member.model.dto.MemberDTO" alias="MemberDTO"/>
        <typeAlias type="com.px.init.member.model.dto.MemberRoleDTO" alias="MemberRoleDTO"/>
        <typeAlias type="com.px.init.member.model.dto.AuthorityDTO" alias="AuthorityDTO"/>
        <typeAlias type="com.px.init.resume.model.dto.ResumeDTO" alias="ResumeDTO"/>
        <typeAlias type="com.px.init.resume.model.dto.SchoolDTO" alias="SchoolDTO"/>
        <typeAlias type="com.px.init.resume.model.dto.CareerDTO" alias="CareerDTO"/>
        <typeAlias type="com.px.init.resume.model.dto.ExternalDTO" alias="ExternalDTO"/>
        <typeAlias type="com.px.init.resume.model.dto.CoreActivityDTO" alias="CoreActivityDTO"/>
        <typeAlias type="com.px.init.resume.model.dto.ForeignLanguageDTO" alias="ForeignLanguageDTO"/>
        <typeAlias type="com.px.init.resume.model.dto.TrainingDTO" alias="TrainingDTO"/>
        <typeAlias type="com.px.init.resume.model.dto.QualificationDTO" alias="QualificationDTO"/>
        <typeAlias type="com.px.init.resume.model.dto.SkillStackDTO" alias="SkillStackDTO"/>
        <typeAlias type="com.px.init.resume.model.dto.DesiredJobDTO" alias="DesiredJobDTO"/>
        <typeAlias type="com.px.init.member.model.dto.PersonalMemberDTO" alias="PersonalMemberDTO"/>
    </typeAliases>

    <mappers>
        <mapper resource="MemberMapper.xml"/>
    </mappers>
</configuration>