                A.ENT_LOCATION,
                B.NUM_EMP,
                B.COM_ADDRESS,
                B.COM_URL,
                A.PREFERENCE,
                B.BENEFITS,
                A.CONTENT,
                A.IS_CLOSE,
                A.POST_START_DATE,
                A.POST_END_DATE,
                A.POSTING_TIME,
